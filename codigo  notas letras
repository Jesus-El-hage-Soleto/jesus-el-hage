#include <iostream>
using namespace std;

int main()
{
char nota;
cout<<"ingrese la nota: ";
cin>>nota;
switch(nota){
    case 'A':
        cout<<"Excelente";
        break;
    case 'B':
        cout<<"Bueno";
        break;
    case 'D':
        cout<<"Esta reprobado ";
        break;
    default:
         cout<<"Error";
    
}
    
    
    return 0;
    
    
    
    
}
